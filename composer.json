{
    "name": "creaminds/eloquent-hashids",
    "description": "On-the-fly hashids for Laravel Eloquent models.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Ali Tavassoli",
            "email": "mtvs.dev@gmail.com"
        }
    ],
    "require": {
        "illuminate/database": "^8.0|^9.0|^10.0",
        "creaminds/laravel-hashids": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.4|^9.0|^10.0",
        "laravel/legacy-factories": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Creaminds\\EloquentHashids\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creaminds\\EloquentHashids\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:creaminds/laravel-hashids.git"
        }
    ]
}
